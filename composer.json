{
  "name": "dnovikov32/http-process-bundle",
  "type": "symfony-bundle",
  "description": "http process bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "league/pipeline": "^1.0",
    "symfony/framework-bundle": "~6.0",
    "symfony/validator": "~6.0",
    "symfony/property-access": "~6.0"
  },
  "autoload": {
    "psr-4": { "Dnovikov32\\HttpProcess\\": "src/" }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    }
  }
}
